  "lint-staged": {
    "**/*.ts?(x)": [
      "stylelint",
      "prettier --write",
      "tslint -p tsconfig.test.json --fix",
      "git add"
    ],
    "**/*.js?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "publishConfig": {
    "registry": "https://nexusrepo.looker.com/repository/looker-npm-private/"
  },
  "scripts": {
    "start": "yarn build-icons && styleguidist server",
    "build": "yarn clean && yarn build-icons && yarn build-components && yarn build-styleguide",
    "build-icons": "ts-node bin/icons/build_icon_components.ts",
    "build-components": "tsc -p tsconfig.build.json && babel src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\" -s",
    "build-styleguide": "styleguidist build",
    "clean": "./ci/clean.sh",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:css": "stylelint \"src/**/*.ts?(x)\"",
    "lint:ts": "tslint -p tsconfig.test.json --format stylish",
    "lint-staged": "yarn exec lint-staged",
    "npmrc": "bin/create_npmrc.sh",
    "npmrc:clean": "rm .npmrc",
    "pretest": "yarn lint",
    "prettify": "yarn prettier \"./**/*.ts?(x)\" \"./**/*.js?(x)\" '!./node_modules/**' '!./dist/**' --write",
    "release": "npm publish",
    "test": "yarn build && yarn tsc -p tsconfig.test.json && yarn jest",
    "test:jest": "yarn build-icons && yarn jest"
  },
  "types": "dist"
}
