{
  "name": "looker-lens",
  "version": "0.0.1-beta.7",
  "main": "dist/index",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@svgr/cli": "3.1.0",
    "@types/d3-color": "1.2.1",
    "@types/d3-hsv": "0.1.2",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "23.3.5",
    "@types/react": "16.4.8",
    "@types/react-dom": "16.0.6",
    "@types/react-intl": "2.3.5",
    "@types/react-resize-detector": "^3.1.0",
    "@types/react-test-renderer": "16.0.1",
    "@types/react-transition-group": "^2.0.14",
    "@types/styled-system": "^3.0.9",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "1.0.0",
    "csstype": "^2.5.7",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "glob": "7.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.2.1",
    "lint-staged": "7.2.0",
    "ora": "3.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.13.7",
    "prop-types": "15.6.2",
    "react-docgen-typescript": "1.10.0",
    "react-styleguidist": "7.3.10",
    "react-test-renderer": "16.5.2",
    "request": "2.85.0",
    "rimraf": "2.6.2",
    "styled-components": "^3.4.9",
    "stylelint": "9.6.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "1.0.0",
    "stylelint-processor-styled-components": "1.5.0",
    "ts-jest": "23.10.4",
    "ts-loader": "5.2.1",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2",
    "typescript-tslint-plugin": "^0.2.1",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.20.2"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0",
    "chroma-js": "^1.3.7",
    "clean-tag": "^2.0.0",
    "d3-color": "^1.2.3",
    "d3-hsv": "^0.1.0",
    "focus-trap-react": "^5.0.1",
    "polished": "2.0.3",
    "popper.js": "^1.14.4",
    "react": "16.5.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "16.4.2",
    "react-hotkeys": "^1.1.4",
    "react-popper": "^1.0.2",
    "react-resize-detector": "^3.2.1",
    "react-scrolllock": "^4.0.1",
    "react-transition-group": "^2.5.1",
    "styled-system": "^3.1.6",
    "svg-inline-loader": "^0.8.0"
  },
  "peerDependencies": {
    "@types/react": "16.4.8",
    "@types/react-dom": "16.0.6",
    "@types/styled-system": "^3.0.9",
    "styled-components": "^3.4.9"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "./test/setupJest.js",
    "transform": {
      "^.+\\.(jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).(ts|js)?(x)"
    ]
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "stylelint",
      "prettier --write",
      "tslint -p tsconfig.test.json --fix",
      "git add"
    ],
    "**/*.js?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "publishConfig": {
    "registry": "https://nexusrepo.looker.com/repository/looker-npm-private/"
  },
  "resolutions": {
    "**/@types/react": "16.4.8"
  },
  "scripts": {
    "start": "yarn build-icons && styleguidist server",
    "build": "yarn build-components && yarn build-styleguide",
    "build-icons": "node bin/icons/build_icon_components.js",
    "build-components": "yarn clean && yarn build-icons && tsc -p tsconfig.build.json && babel src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\" -s",
    "build-styleguide": "yarn clean && yarn build-icons && styleguidist build && tar -zcvf lens-styleguide.tar.gz styleguide && rimraf styleguide",
    "clean": "rimraf dist && rimraf styleguide && rimraf src/icons/build && rimraf lens-styleguide.tar.gz",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:css": "stylelint \"src/**/*.ts?(x)\"",
    "lint:ts": "tslint -p tsconfig.test.json --format stylish",
    "lint-staged": "$(yarn bin)/lint-staged",
    "npmrc": "bin/create_npmrc.sh",
    "npmrc:clean": "rm .npmrc",
    "prepublishOnly": "yarn clean && yarn && yarn test && yarn build-components",
    "prettify": "yarn prettier \"./**/*.ts?(x)\" \"./**/*.js?(x)\" '!./node_modules/**' '!./dist/**' --write",
    "release": "npm publish",
    "test": "yarn build-icons && tsc -p tsconfig.test.json && yarn jest"
  },
  "types": "dist"
}
